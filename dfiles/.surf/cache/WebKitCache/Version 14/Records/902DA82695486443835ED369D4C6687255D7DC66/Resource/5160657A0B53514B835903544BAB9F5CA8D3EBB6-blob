            if (navigator && navigator.sendBeacon && !isIphoneMshop) {
                navigator.sendBeacon(metricEndpoint, sushiPayload);
            } else {
                A.post(metricEndpoint, options);
            };

            var errorHandler = function () {
                emitSushiMetric(sushiPayload, metricEndpoint, ++retryCount);
            };
        }
