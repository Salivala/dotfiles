    window.$Nav && $Nav.when('$').run('CAMarketplaceRedirectOverlay',function($) {
        $.ajax({
            type: 'POST',
            url: '/gp/redirection/canada.html',
            data: {
                path: '/V-MODA-Crossfade-Over-Ear-Noise-Isolating-Headphone/dp/B00HYH7HXA',
                queryString: '?ie=UTF8\u0026s=gateway\u0026keywords=vmoda\u0026qid=1559960427\u0026sr=8-4',
                pageType: 'Detail',
                referer: 'https://www.amazon.com/s?k=vmoda\u0026ref=nb_sb_noss_2'
            },
            success: function(data) {
                if (data) {
                    $('body').append(data);
                }
            }
        });
    });
</script>










