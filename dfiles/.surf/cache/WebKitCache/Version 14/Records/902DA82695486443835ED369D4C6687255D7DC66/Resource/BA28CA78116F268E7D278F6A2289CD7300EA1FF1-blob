            data: {
                path: '/Pro-NET-Core-Andrew-Troelsen/dp/1484230175',
                queryString: '?ie=UTF8\u0026s=gateway\u0026keywords=pro%20c%23%207\u0026qid=1560129548\u0026psc=1\u0026sr=8-1-spons',
                pageType: 'Detail',
                referer: 'https://www.amazon.com/s?k=pro+c%23+7\u0026ref=nb_sb_noss_2'
            },
            success: function(data) {
