last=i}else{first=last=CommentVotes.last_click=i
CommentVotes.checked[i].on=!CommentVotes.checked[i].on;}
var on=CommentVotes.checked[first].on
if(!event.ctrlKey){CommentVotes.set(0,CommentVotes.checked.length-1,false)}
CommentVotes.set(first,last,on)
CommentVotes.update()
event.stopPropagation();event.preventDefault();},mouseover:function(id,event){var i=CommentVotes.get_row_by_id(id)
if(!i)return;if(CommentVotes.last_click==-1){CommentVotes.last_click=i}
