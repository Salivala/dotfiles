function ajaxResponseWrap(success,failure){return function(resp){if(resp.request.success()){return success(resp);}
if(failure){failure(resp);}
var jsonResponse=resp.responseJSON;if(jsonResponse&&('reason'in jsonResponse)){var message=jsonResponse.reason;if('message'in jsonResponse)
message+='. '+jsonResponse.message;error('Error: '+message);}else{error('Error: '+resp.statusText);}}}
function setSelectionRange(input,selectionStart,selectionEnd){if('selectionStart'in input){input.focus();input.selectionStart=selectionStart;input.selectionEnd=selectionEnd;}else if(input.setSelectionRange){input.focus();input.setSelectionRange(selectionStart,selectionEnd);}else if(input.createTextRange){input.focus();var range=input.createTextRange();range.collapse(true);range.moveEnd('character',selectionEnd);range.moveStart('character',selectionStart);range.select();}}
function setCaretToPos(input,pos){setSelectionRange(input,pos,pos);}
