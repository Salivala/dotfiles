pressedF=false;return false;}});});jQuery(document).ready(function(){if(Cookie.get("css").split(";")[1]=="overrides/spring"&&jQuery("#image").length>0&&jQuery("#image").attr("alt").match(/\bfennec\b/)){jQuery("#content").css("background-image","url('/images/content_bg/spring_bottom.png'), url('/images/content_bg/spring_bottom_fennec.png'), url('/images/content_bg/spring_top.png'), url('/images/content_bg/spring_top_sky.png')");jQuery("#user_override_css").change(function(){jQuery("#content").css("background-image","");});}});