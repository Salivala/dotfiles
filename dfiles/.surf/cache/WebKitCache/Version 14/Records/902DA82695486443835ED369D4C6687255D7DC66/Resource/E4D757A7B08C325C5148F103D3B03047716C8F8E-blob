function hideCategory(category)
{$$(".tag-type-"+category).each(function(e){e.style.display="none";});$("category-"+category).hide();$("category-hidden-"+category).show();}
function showCategory(category)
{$$(".tag-type-"+category).each(function(e){e.style.display="list-item";});$("category-"+category).show();$("category-hidden-"+category).hide();}
