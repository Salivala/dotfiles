if($("p"+post_id))
$("p"+post_id).remove()
if($("set"+set_id))
$("set"+set_id).remove()})})}}
function fillShortName()
{var name=jQuery("#set_name").val();var shortname=name.toLowerCase().replace(/ /g,"").replace(/[^a-zA-Z0-9_]/g,"");if(!window.shortNameEdited)
jQuery("#set_shortname").val(shortname);jQuery("#shortname_link").html("<a href='/post?tags=set:"+jQuery("#set_shortname").val()+"'>set:"+jQuery("#set_shortname").val()+"</a>");}
function shortNameChanged()
{window.shortNameEdited=true;jQuery("#shortname_link").html("<a href='/post?tags=set:"+jQuery("#set_shortname").val()+"'>set:"+jQuery("#set_shortname").val()+"</a>");}
FlagReason={destroy:function(id){notice('Deleting flag reason #'+id)
new Ajax.Request('/flag_reason/destroy.json',{parameters:{"id":id},onComplete:ajaxResponseWrap(function(resp){notice("Flag reason deleted")
jQuery("#flag-reason-"+id).fadeOut("fast");})})}}
Takedown={destroy:function(id){notice('Deleting takedown #'+id+"...");jQuery.ajax({url:"/takedown/destroy.json",type:"POST",dataType:"json",headers:{accept:"text/javascript"},data:{"id":id}}).done(function(data){notice("Takedown deleted");jQuery("#takedown-"+id).fadeOut("fast");}).error(function(data){error(data.responseText);});},add_posts_by_tags_preview:function(id,tags){jQuery.ajax({url:"/takedown/count_matching_posts.json",type:"POST",dataType:"json",headers:{accept:"text/javascript"},data:{"id":id,"tags":tags}}).done(function(data){var count=data.matched_post_count;var preview_text="<a href='/post/index?tags="+tags.replace(" ","+")+"+status:any'>"+count+" "+(count==1?"post":"posts")+"</a> "+(count==1?"matches":"match")+" the search '<a href='/post/index?tags="+tags.replace(" ","+")+"+status:any'>"+tags+"</a>'. Click Confirm to add "+(count==1?"it":"them")+" to the takedown.";jQuery("#takedown-add-posts-tags-warning").html(preview_text).show();jQuery("#takedown-add-posts-tags").prop("disabled",true);jQuery("#takedown-add-posts-tags-preview").hide();jQuery("#takedown-add-posts-tags-confirm").show();jQuery("#takedown-add-posts-tags-cancel").show();}).error(function(data){error(data.responseText);});},add_posts_by_tags_cancel:function(){jQuery("#takedown-add-posts-tags-warning").hide();jQuery("#takedown-add-posts-tags").val('').prop("disabled",false);jQuery("#takedown-add-posts-tags-preview").show().prop("disabled",true);jQuery("#takedown-add-posts-tags-confirm").hide();jQuery("#takedown-add-posts-tags-cancel").hide();},add_posts_by_tags:function(id,tags){notice("Adding posts with tags '"+tags+"' to takedown...");jQuery.ajax({url:"/takedown/add_posts_by_tags.json",type:"POST",dataType:"json",headers:{accept:"text/javascript"},data:{"id":id,"tags":tags}}).done(function(data){var added_post_ids=data.added_post_ids;var count=added_post_ids.length;notice(count+" post"+(count==1?"":"s")+" with tags '"+tags+"' added to takedown");for(var i=0;i<count;i++){var html=Takedown.post_button_html(added_post_ids[i]);jQuery(html).appendTo(jQuery("#takedown-post-buttons"));}
jQuery("#takedown-add-posts-tags-submit").prop("disabled",true);Takedown.add_posts_by_tags_cancel();}).error(function(data){error(data.responseText);});},add_posts_by_ids:function(id,post_ids){notice("Adding posts to takedown...");jQuery.ajax({url:"/takedown/add_posts_by_ids.json",type:"POST",dataType:"json",headers:{accept:"text/javascript"},data:{"id":id,"post_ids":post_ids}}).done(function(data){notice(data.added_count+" post"+(data.added_count==1?"":"s")+" added to takedown");var added_post_ids=data.added_post_ids;for(var i=0;i<added_post_ids.length;i++){var html=Takedown.post_button_html(added_post_ids[i]);jQuery(html).appendTo(jQuery("#takedown-post-buttons"));}
jQuery("#takedown-add-posts-ids").val('');jQuery("#takedown-add-posts-ids-submit").prop("disabled",true);}).error(function(data){error(data.responseText);});},remove_post:function(id,post_id){notice("Removing post #"+post_id+" from takedown...");jQuery.ajax({url:"/takedown/remove_post.json",type:"POST",dataType:"json",headers:{accept:"text/javascript"},data:{"id":id,"post_id":post_id}}).done(function(data){notice("Post #"+post_id+" removed from takedown")
jQuery("#takedown-post-"+post_id).remove();}).error(function(data){error(data.responseText);});},post_button_html:function(post_id){return"<div id='takedown-post-"+post_id+"' data-post-id='"+post_id+"' class='takedown-post'><div class='takedown-post-label takedown-post-remove' title='Remove this post from the takedown'>X</div> <label for='takedown_posts_"+post_id+"' class='takedown-post-label takedown-post-keep'><input name='takedown_posts["+post_id+"]' type='hidden' value='0'><input id='takedown_posts_"+post_id+"' name='takedown_posts["+post_id+"]' type='checkbox' value='1'> <span>Keep</span></label> <a href='/post/show/"+post_id+"'>post #"+post_id+"</a></div>";}}
jQuery(document).ready(function(){var pressedF=false;jQuery(window).keydown(function(e){if(e.which==70)pressedF=true;}).keyup(function(e){if(e.which==70)pressedF=false;})
jQuery("#morelink").click(function(e){if(pressedF&&e.shiftKey)
{var left=859;if(jQuery("#eegg-tony-fox").length>0)
jQuery("#eegg-tony-fox").animate({left:"-"+left+"px",opacity:0},3000,"swing",function(){jQuery("#eegg-tony-fox").remove()});else
{var scale=Math.round((jQuery(window).height()/816)*100)/100;var scalestr="";if(scale<1)
{scalestr="transform:scale("+scale+");-o-transform:scale("+scale+");-moz-transform:scale("+scale+");-webkit-transform:scale("+scale+");transform-origin:0% 100%;-o-transform-origin:0% 100%;-moz-transform-origin:0% 100%;-webkit-transform-origin:0% 100%;";left=Math.round(859*scale);}
