++ret}
return ret;},get_first_selected_row:function(){for(i=0;i<CommentVotes.checked.length;++i){if(CommentVotes.checked[i].on)
return i;}
return null;},get_row_by_id:function(id){for(i=0;i<CommentVotes.checked.length;++i){if(CommentVotes.checked[i].id==id)
return i;}
return null;},set:function(first,last,on){i=first;while(true)
{CommentVotes.checked[i].on=on;if(i==last)
break;i+=(last>first)?+1:-1;}},doc_mouseup:function(event){CommentVotes.dragging=false
document.stopObserving("mouseup",CommentVotes.doc_mouseup)},mouse_is_down:function(){CommentVotes.dragging=true;document.observe("mouseup",CommentVotes.doc_mouseup)},mousedown:function(id,event){if(!Event.isLeftClick(event)){return;}
