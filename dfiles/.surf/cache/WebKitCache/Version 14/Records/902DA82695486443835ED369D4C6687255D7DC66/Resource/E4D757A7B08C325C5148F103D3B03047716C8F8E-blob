function countInput(div,min,max,countDiv){if(countDiv=="blacklist-length")
{var text=unescape(encodeURIComponent(div.value.replace(/\r\n/g,'\n')));var length=text.length;$(countDiv).innerHTML=length;}
else
{var length=div.value.length;$(countDiv).innerHTML=length;}
if(length<min||length>max)$(countDiv).className="redtext";else $(countDiv).className="";}
function checkBlacklist(){new Ajax.Request("/user/check_blacklist.json",{method:"post",parameters:{"blacklist":$('user_blacklisted_tags').value,"authenticity_token":jQuery("input[name='authenticity_token']").val()},onSuccess:function(resp){var resp=resp.responseJSON;if(resp.length>3900)$('blacklist-length').className="redtext"
else $('blacklist-length').className=""
$('blacklist-length').innerHTML=resp.length
$('user_blacklisted_tags').value=resp.blacklist
user_blacklisted_tags.highlight();},onFailure:function(resp){error("Error: "+resp.reason+$('user_blacklisted_tags').value)}});}
function sortBlacklist(){blacklist=$("user_blacklisted_tags").value;$("user_blacklisted_tags").value=alphebetize(blacklist);checkBlacklist()}
function alphebetize(str){return str.split("\n").sort().join("\n")}
function toggleCSSDropdown(){if(jQuery("#user-css-inner").is(":visible")){jQuery("#user-css-inner").hide();jQuery("#user-css-hide").hide();jQuery("#user-css-show").show();}
else{jQuery("#user-css-inner").show();jQuery("#user-css-hide").show();jQuery("#user-css-show").hide();}}
jQuery(document).click(function(e){if(e.target.id!="user-css-hide"&&e.target.id!="user-css-show"&&jQuery(e.target).parents("#user-css-inner").length==0){jQuery("#user-css-inner").hide();jQuery("#user-css-hide").hide();jQuery("#user-css-show").show();}
if(jQuery("#child-posts").length>0&&jQuery("#child-posts").hasClass("child-posts-expanded")&&e.target.id!="child-posts"&&jQuery(e.target).parents("#child-posts").length==0){toggleChildPosts();}});function cssChanged(){var cssStr=jQuery("#user_css").val();var cssOverrideStr=jQuery("#user_override_css").val();var cssFiles=cssStr.split(",");Cookie.put("css",cssStr+";"+cssOverrideStr+";"+cssOverrideUpdated);updateCSS(cssFiles,cssOverrideStr);}
jQuery(function(){jQuery("#user_ignore_theme_changes").change(function(){var form=jQuery(this)[0].form;jQuery.post('/user/update',jQuery(form).serialize(),'post');});});function loadCSS(){var cookieStr=Cookie.get("css");var cssStr=cookieStr.split(";")[0];var cssOverrideStr=cookieStr.split(";")[1];var cssSetTime=cookieStr.split(";")[2];if(cssSetTime!=cssOverrideUpdated&&!user_ignore_theme_changes){cssStr=Cookie.get("css").split(";")[0];Cookie.put("css",cssStr+";"+cssOverrideDefault+";"+cssOverrideUpdated);}
if(getParam("theme"))cssStr=getParam("theme");if(getParam("theme-extra"))cssOverrideStr=getParam("theme-extra");if(cssStr){var cssFiles=cssStr.split(",");jQuery("#user_css").val(cssStr);jQuery("#user_override_css").val(cssOverrideStr);updateCSS(cssFiles,cssOverrideStr);}}
function updateCSS(cssFiles,cssOverrideStr){for(var i=0;i<cssFiles.length;i++){if(jQuery(".main-stylesheet").eq(i).length==0)
jQuery("head").append("<link rel='stylesheet' type='text/css' href='/stylesheets/"+cssFiles[i]+".css' class='main-stylesheet' media='screen'>")
else if(!jQuery(".main-stylesheet").eq(i).attr("href").match("/stylesheets/"+cssFiles[i]+".css"))
jQuery(".main-stylesheet").eq(i).attr("href","/stylesheets/"+cssFiles[i]+".css");}
if(cssOverrideStr=="none"){jQuery(".override-stylesheet").remove();}
else{if(jQuery(".override-stylesheet").length==0)
jQuery("head").append("<link rel='stylesheet' type='text/css' href='/stylesheets/"+cssOverrideStr+".css' class='override-stylesheet' media='screen'>")
else if(!jQuery(".override-stylesheet").attr("href").match("/stylesheets/"+cssOverrideStr+".css"))
jQuery(".override-stylesheet").attr("href","/stylesheets/"+cssOverrideStr+".css");}
var mainStylesheets=jQuery(".main-stylesheet");for(var i=cssFiles.length;i<mainStylesheets.length;i++){mainStylesheets[i].remove();}}
function getParam(name){name=name.replace(/[\[]/,"\\[").replace(/[\]]/,"\\]");var regex=new RegExp("[\\?&]"+name+"=([^&#]*)"),results=regex.exec(location.search);return results==null?"":decodeURIComponent(results[1].replace(/\+/g," "));}
function notice(msg){var wasVisible=jQuery("#notice").is(":visible")||jQuery("#error").is(":visible")||jQuery("#warning").is(":visible")
jQuery("#notice").html(msg+"<div class='closebutton' onclick='noticeClose(this.parentNode);'>X</div>").show()
jQuery("#error, #warning").hide();if(jQuery("#content").length==0)return;if(!wasVisible&&jQuery(document).scrollTop()>noticeOffset)jQuery(window).scrollTop(jQuery(window).scrollTop()+42);else noticeCheckScroll();}
function error(msg){var wasVisible=jQuery("#notice").is(":visible")||jQuery("#error").is(":visible")||jQuery("#warning").is(":visible")
jQuery("#error").html(msg+"<div class='closebutton' onclick='noticeClose(this.parentNode);'>X</div>").show()
jQuery("#notice, #warning").hide();if(jQuery("#content").length==0)return;if(!wasVisible&&jQuery(document).scrollTop()>noticeOffset)jQuery(window).scrollTop(jQuery(window).scrollTop()+42);else noticeCheckScroll();}
function warning(msg){var wasVisible=jQuery("#notice").is(":visible")||jQuery("#error").is(":visible")||jQuery("#warning").is(":visible")
jQuery("#warning").html(msg+"<div class='closebutton' onclick='noticeClose(this.parentNode);'>X</div>").show()
jQuery("#notice, #error").hide();if(jQuery("#content").length==0)return;if(!wasVisible&&jQuery(document).scrollTop()>noticeOffset)jQuery(window).scrollTop(jQuery(window).scrollTop()+42);else noticeCheckScroll();}
function noticeClose(bar){jQuery(bar).hide();if(jQuery("#content").length==0)return;if(!jQuery("#notice").is(':visible')&&!jQuery("#error").is(':visible')&&!jQuery("#warning").is(':visible'))
jQuery("#notices").removeClass("fixed");jQuery(window).scrollTop(jQuery(window).scrollTop()-42);}
function noticeCheckScroll(){if(jQuery("#content").length==0)return;if(!jQuery("#notice").is(':visible')&&!jQuery("#error").is(':visible')&&!jQuery("#warning").is(':visible'))
return;if(jQuery(document).scrollTop()>noticeOffset)
jQuery("#notices").addClass('fixed');else
jQuery("#notices").removeClass('fixed').width("");}
jQuery(function(){if(jQuery("#content").length==0)return;var shown=jQuery("#notice").is(":visible");jQuery("#notice").show();noticeOffset=Math.max(0,jQuery('#notice')[0].offsetTop-15);if(!shown)jQuery("#notice").hide();jQuery(document).bind('ready scroll',function(){noticeCheckScroll();});});jQuery.fn.highlight=function(){jQuery(this).each(function(){var el=jQuery(this);el.before("<div/>")
el.prev().width(el.width()).height(el.height()).css({"position":"absolute","background-color":"#ffffff","opacity":".5"}).fadeOut(1000);});};(function(){var now=new Date();if(now.getDate()===1&&now.getMonth()===3&&Math.random()<0.15){document.write('<div id="dp"></div>');document.write('<link href=\'/stylesheets/overrides/deadpixel.css\' media=\'screen\' rel=\'stylesheet\' type=\'text/css\'>');}})();Cookie={put:function(name,value,days){if(days==null){days=365}
var date=new Date()
date.setTime(date.getTime()+(days*24*60*60*1000))
var expires="; expires="+date.toGMTString()
document.cookie=name+"="+encodeURIComponent(value)+expires+"; path=/"},raw_get:function(name){var nameEq=name+"="
var ca=document.cookie.split(";")
for(var i=0;i<ca.length;++i){var c=ca[i]
while(c.charAt(0)==" "){c=c.substring(1,c.length)}
if(c.indexOf(nameEq)==0){return c.substring(nameEq.length,c.length)}}
return""},get:function(name){return this.unescape(this.raw_get(name))},remove:function(name){Cookie.put(name,"",-1)},unescape:function(val){return decodeURIComponent(val.replace(/\+/g," "))},setup:function(){if(location.href.match(/^\/(comment|pool|note|post)/)&&this.get("tos")!="1"){var domain=location.href.match(/^(http:\/\/[^\/]+)/)[0]
