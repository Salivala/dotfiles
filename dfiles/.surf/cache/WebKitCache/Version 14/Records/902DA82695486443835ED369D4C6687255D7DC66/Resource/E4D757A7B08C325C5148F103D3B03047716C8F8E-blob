var PostQueue=(function(){function Queue(timer){this.timeout=timer||1000;this.queue=[];this.id=null;this.running=true;}
Queue.prototype.tick=function(){if(!this.running||!this.queue.length){clearInterval(this.id);this.id=null;return this;}
this.queue.shift()();return this;};Queue.prototype.add=function(callback){this.queue.push(callback);if(this.running){this.start();}
return this;};Queue.prototype.start=function(){var self=this;this.running=true;if(!this.id){this.id=setInterval(function(){self.tick();},this.timeout);this.tick();}};Queue.prototype.stop=function(){if(this.id){clearInterval(this.id);}
this.id=null;this.running=false;};return Queue;}());Post={mod_queue:null,posts:new Hash(),pending_update_count:0,blacklist_options:{replace:true},sendQueue:new PostQueue(700),notice_update:function(increase_or_decrease){if(increase_or_decrease=="inc"){Post.pending_update_count+=1
